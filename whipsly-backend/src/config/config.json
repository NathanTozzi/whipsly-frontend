{
  "development": {
    "username": "postgres",
    "password": "password",
    "database": "whipsly_dev",
    "host": "localhost",
    "port": 5432,
    "dialect": "postgres",
    "logging": true,
    "define": {
      "timestamps": true,
      "underscored": true,
      "freezeTableName": false
    },
    "pool": {
      "max": 5,
      "min": 0,
      "acquire": 30000,
      "idle": 10000
    }
  },
  "test": {
    "username": "postgres",
    "password": "password",
    "database": "whipsly_test",
    "host": "localhost",
    "port": 5432,
    "dialect": "postgres",
    "logging": false,
    "define": {
      "timestamps": true,
      "underscored": true,
      "freezeTableName": false
    }
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    },
    "logging": false,
    "define": {
      "timestamps": true,
      "underscored": true,
      "freezeTableName": false
    },
    "pool": {
      "max": 10,
      "min": 2,
      "acquire": 30000,
      "idle": 10000
    }
  }
}